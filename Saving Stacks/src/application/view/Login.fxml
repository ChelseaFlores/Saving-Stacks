<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoginController">
   <children>
      <Pane layoutX="-1.0" layoutY="-4.0" prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: FFFFFF;">
         <children>
            <ImageView fitHeight="306.0" fitWidth="416.0" layoutX="185.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../data/savingstacks.png" />
               </image>
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
            </ImageView>
            <Pane layoutX="121.0" layoutY="350.0" prefHeight="390.0" prefWidth="544.0" style="-fx-background-color: #264d73;">
               <children>
                  <PasswordField layoutX="102.0" layoutY="125.0" prefHeight="30.0" prefWidth="341.0">
                     <effect>
                        <Glow />
                     </effect>
                  </PasswordField>
                  <Label layoutX="31.0" layoutY="73.0" text="Please Enter Your Password:" textFill="#f2eded">
                     <font>
                        <Font name="Bodoni MT" size="23.0" />
                     </font>
                  </Label>
                  <Button layoutX="242.0" layoutY="180.0" mnemonicParsing="false" text="Login" />
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <effect>
                  <DropShadow />
               </effect>
            </Pane>
         </children></Pane>
   </children>
</AnchorPane>
