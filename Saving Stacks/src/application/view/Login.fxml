<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoginController">
   <children>
      <Pane fx:id="loginAnchor" layoutX="-1.0" layoutY="-4.0" prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: FFFFFF;">
         <children>
            <ImageView fitHeight="276.0" fitWidth="365.0" layoutX="63.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../data/logo.png" />
               </image>
               <effect>
                  <Lighting>
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
            </ImageView>
            <PasswordField fx:id="passwordField" layoutX="192.0" layoutY="361.0" prefHeight="46.0" prefWidth="416.0" promptText="Please Enter Your Password:" style="-fx-background-color: F5F5F5; -fx-background-radius: 30;">
               <font>
                  <Font size="18.0" />
               </font>
            </PasswordField>
            <Button fx:id="login" layoutX="445.0" layoutY="437.0" mnemonicParsing="false" onAction="#handle" prefHeight="46.0" prefWidth="156.0" style="-fx-background-color: black; -fx-background-radius: 30;" text="Login" textFill="WHITE">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="forgotPassword" layoutX="199.0" layoutY="437.0" mnemonicParsing="false" onAction="#handleForgotPassword" prefHeight="46.0" prefWidth="156.0" style="-fx-background-color: black; -fx-background-radius: 30;" text="Forgot Password" textFill="WHITE">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Label fx:id="greeting" layoutX="201.0" layoutY="266.0" prefHeight="62.0" prefWidth="259.0" text="Hello, ">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Label fx:id="name" layoutX="295.0" layoutY="266.0" prefHeight="62.0" prefWidth="305.0" text="Name">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label layoutX="349.0" layoutY="108.0" prefHeight="78.0" prefWidth="385.0" text="Saving Stacks">
               <font>
                  <Font name="Bodoni MT Bold" size="63.0" />
               </font>
               <effect>
                  <Lighting bumpInput="$null">
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
            </Label>
            <Label fx:id="forgotPasswordLabel" layoutX="128.0" layoutY="503.0" prefHeight="52.0" prefWidth="545.0" textAlignment="CENTER">
               <font>
                  <Font name="Bodoni MT" size="18.0" />
               </font>
            </Label>
            <Label fx:id="questionLabel" layoutX="128.0" layoutY="541.0" prefHeight="52.0" prefWidth="545.0">
               <font>
                  <Font name="Bodoni MT" size="18.0" />
               </font>
            </Label>
         </children></Pane>
   </children>
</AnchorPane>
